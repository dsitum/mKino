<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
	<style name="myBackgroundStyle" parent="@android:style/Theme.Dialog">
        <item name="android:background">#FF0000</item>
    </style>
    
    <!-- Application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <color name="backgroundDark">#1d1d1d</color>
	<color name="textWhite">#ffffffd5</color>
	<color name="greenDark">#4f2958</color>
	<color name="greenLight">#23b6a3</color>
	<color name="blueLight">#666666</color>
	<color name="backgroundMedium">#444242</color>
	
</resources>
